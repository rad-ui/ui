{
  "sourceType": "unambiguous",
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "targets": {
          "chrome": 100,
          "safari": 15,
          "firefox": 91
        },

      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@": "./src"
      }
    }]
  ]
}