{
    "sourceType": "unambiguous",
    "presets": [
        "@babel/preset-typescript",
        "@babel/preset-react",
        "@babel/preset-env"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "@": "./src"
                }
            }
        ]
    ],
    "ignore": [
        "docs/**"
    ]
}
