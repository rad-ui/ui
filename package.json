  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "clean": "rimraf dist",
    "chromatic": "chromatic --exit-zero-on-changes",
    "patch": "npm version patch",
    "sb": "storybook dev -p 6006",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-css": "parcel build styles/themes/default.scss --out-dir dist/themes/",
    "generate-tokens": "node ./scripts/tokenGen/index.js",
    "bundle-tokens": "rollup --config rollup-tokens.config.js",
    "build-components": "rollup -c --bundleConfigAsCjs",
    "copy-components": "mkdirp ./dist/components && cp -r ./dist/temp-cleanup/*js ./dist/components && rimraf dist/temp-cleanup",
    "build": "npm run test && npm run clean && npm run build-css && npm run generate-tokens && npm run bundle-tokens && npm run build-components && npm run copy-components",
    "release": "npm publish --access public",
    "release:patch": "npm run patch && npm run release",
    "release:minor": "npm version minor && npm run release",
    "release:major": "npm version major && npm run release",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "update-rad": "npm update @radui/ui",
    "setup:dependencies": "npm install --force",
    "setup": "npm run setup:dependencies"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rad-ui/ui"
  },
