{
  "name": "@radui/ui",
  "version": "0.0.47",
  "description": "",
  "main": "dist",
  "type": "module",
  "exports": {},
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "clean": "rimraf dist",
    "chromatic": "chromatic --exit-zero-on-changes",
    "patch": "npm version patch",
    "sb": "storybook dev -p 6006",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "storybook test --index-json ./storybook-static/index.json",
    "build-css": "rollup -c rollup-css.config.js",
    "generate-tokens": "tsx ./src/tokenGen/index.js",
    "bundle-tokens": "rollup --config rollup-tokens.config.js",
    "compile-components": "rollup -c rollup.config.cjs",
    "process-components": "node scripts/process-components.cjs",
    "prebuild": "npm run test && npm run clean",
    "build:generate-exports": "node scripts/generate-exports.cjs",
    "build:css": "npm run generate-tokens && npm run bundle-tokens && npm run build-css",
    "build:components": "npm run compile-components && npm run process-components",
    "build:rollup": "NODE_OPTIONS='--max-old-space-size=8192' npm run build:rollup:process",
    "build:rollup:process": "npm run prebuild && (npm run build:css & npm run build:components & wait) && npm run build:generate-exports",
    "compile-components:esbuild": "node build-components.cjs",
    "check:types": "tsc --noEmit",
    "build-components:esbuild": "npm run compile-components:esbuild && npm run process-components",
    "build:esbuild": "npm run prebuild && npm run build:css && npm run build-components:esbuild",
    "release": "npm publish --access public",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx . --fix && stylelint \"**/*.{css,scss,sass}\" --fix",
    "lint:staged": "lint-staged",
    "update-rad": "npm update @radui/ui",
    "setup:dependencies": "npm install --force",
    "setup": "npm run setup:dependencies"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss,sass}": [
      "stylelint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rad-ui/ui"
  },
  "bugs": {
    "url": "https://github.com/rad-ui/ui/issues"
  },
  "author": "Pranay Kothapalli",
  "license": "ISC",
  "devDependencies": {
    "@babel/eslint-parser": "7.26.10",
    "@babel/preset-env": "7.24.0",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "^7.24.7",
    "@radui/ui": "^0.0.37",
    "@rollup/plugin-alias": "5.1.0",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@storybook/addon-a11y": "^7.6.17",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-interactions": "8.0.9",
    "@storybook/addon-links": "8.6.12",
    "@storybook/addon-onboarding": "1.0.11",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/blocks": "8.1.10",
    "@storybook/react": "7.6.17",
    "@storybook/react-webpack5": "7.6.17",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.0.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "7.0.0",
    "autoprefixer": "10.4.15",
    "axe-core": "^4.10.2",
    "babel-plugin-module-resolver": "5.0.2",
    "esbuild": "^0.25.6",
    "eslint": "8.56.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.6.0",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "^16.1.2",
    "mkdirp": "^3.0.1",
    "postcss": "8.5.3",
    "rimraf": "6.0.1",
    "rollup": "4.46.2",
    "rollup-plugin-banner2": "1.3.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.77.2",
    "sass-loader": "16.0.4",
    "storybook": "^8.6.11",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "3.4.1",
    "ts-jest": "29.4.0",
    "tsx": "^4.17.0",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.4",
    "clsx": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-remove-scroll": "^2.7.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
